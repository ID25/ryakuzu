- @view.each do |table, column|
  h2= table
  = form_for 'schema', url: :update_hash do |f|
    = text_field 'table_name', table, placeholder: table
    = f.submit
  - column.each do |c|
    p= c
    = form_for 'schema', url: :update_hash do |f|
      = text_field   'column_info', c, placeholder: c
      = hidden_field 'table_info',  table
      = f.submit
